{
  "name": "vscode-min-node",
  "displayName": "VSCode Min-Node",
  "description": "Detects the minimum node version for your project.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:package.json",
    "onCommand:min-node.calculate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "min-node.calculate",
        "title": "Node: Get minimum required version"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.8",
    "@types/vscode": "^1.51.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.5",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "min-node": "^0.4.0"
  }
}
